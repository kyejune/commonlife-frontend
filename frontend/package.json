{
    "name": "app.commonlife",
    "displayName": "commonlife",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "src/index.js",
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "scripts": {
        "build-css": "node-sass-chokidar src/App.scss -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/App.scss -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "npm-run-all -p watch-css start-js",
        "build-js": "react-scripts build",
        "build": "npm-run-all build-css build-js && rm -rf www && mv build www",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "convert-time": "^0.3.0",
        "cordova-android": "^7.0.0",
        "cordova-ios": "^4.5.4",
        "cordova-plugin-calendar": "^5.0.0",
        "cordova-plugin-camera": "^4.0.2",
        "cordova-plugin-device": "^2.0.1",
        "cordova-plugin-file": "^6.0.1",
        "cordova-plugin-file-transfer": "^1.7.1",
        "cordova-plugin-keyboard": "^1.2.0",
        "cordova-plugin-whitelist": "^1.3.3",
        "cordova-plugin-x-socialsharing": "^5.2.1",
        "es6-promise-plugin": "^4.1.0",
        "rc-slider": "^8.6.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-imageloader": "^3.0.0",
        "react-scripts": "1.1.0"
    },
    "devDependencies": {
        "axios": "^0.17.1",
        "classnames": "^2.2.5",
        "font-awesome": "^4.7.0",
        "history": "^4.7.2",
        "immutable": "^3.8.2",
        "material-design-icons": "^3.0.1",
        "mobx": "^3.4.1",
        "mobx-react": "^4.3.5",
        "moment": "^2.21.0",
        "node-sass": "^4.7.2",
        "node-sass-chokidar": "^0.0.3",
        "npm-run-all": "^4.1.2",
        "query-string": "^5.0.1",
        "react-date-range": "^0.9.4",
        "react-day-picker": "^7.0.7",
        "react-draggable": "^3.0.5",
        "react-id-swiper": "^1.6.1",
        "react-md": "^1.2.10",
        "react-router-dom": "^4.2.2",
        "react-sortable": "^2.0.0",
        "react-sortable-hoc": "^0.6.8",
        "simple-json-filter": "^0.3.0"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-file-transfer": {},
            "cordova-plugin-camera": {},
            "cordova-plugin-x-socialsharing": {},
            "cordova-plugin-calendar": {
                "CALENDAR_USAGE_DESCRIPTION": " "
            },
            "cordova-plugin-device-name": {},
            "cordova-plugin-device": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-keyboard": {}
        },
        "platforms": [
            "android",
            "ios"
        ]
    },
    "homepage": "./"
}
